<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>servlet.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>servlet.logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>agregarPaciente</servlet-name>
        <servlet-class>servlet.agregarPaciente</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>agregarAtencion</servlet-name>
        <servlet-class>servlet.agregarAtencion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>consultarPaciente</servlet-name>
        <servlet-class>servlet.consultarPaciente</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>consultarAtencion</servlet-name>
        <servlet-class>servlet.consultarAtencion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>consultarDentista</servlet-name>
        <servlet-class>servlet.consultarDentista</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>modificarAtencion</servlet-name>
        <servlet-class>servlet.modificarAtencion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>modificarPaciente</servlet-name>
        <servlet-class>servlet.modificarPaciente</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>agregarPaciente</servlet-name>
        <url-pattern>/agregarPaciente</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>agregarAtencion</servlet-name>
        <url-pattern>/agregarAtencion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>consultarPaciente</servlet-name>
        <url-pattern>/consultarPaciente</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>consultarAtencion</servlet-name>
        <url-pattern>/consultarAtencion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>consultarDentista</servlet-name>
        <url-pattern>/consultarDentista</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>modificarAtencion</servlet-name>
        <url-pattern>/modificarAtencion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>modificarPaciente</servlet-name>
        <url-pattern>/modificarPaciente</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>login</welcome-file>
    </welcome-file-list>
</web-app>
